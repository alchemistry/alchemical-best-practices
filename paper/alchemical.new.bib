@article{deflorian2020accurate,
    author = "Deflorian, Francesca and Perez-Benito, Laura and Lenselink, Eelke B and Congreve, Miles and van Vlijmen, Herman W. T. and Mason, Jonathan S. and Graaf, Chris de and Tresadern, Gary",
    title = "Accurate Prediction of GPCR Ligand Binding Affinity with Free Energy Perturbation",
    year = "2020",
    month = "June",
    publisher = "{American Chemical Society}",
    issn = "1549-9596",
    doi = "10.1021/acs.jcim.0c00449",
    abstract = "The computational prediction of relative binding free energies is a crucial goal for drug discovery, and G protein-coupled receptors (GPCRs) are arguably the most important drug target class. However, they present increased complexity to model compared to soluble globular proteins. Despite breakthroughs, experimental X-ray crystal and cryo-EM structures are challenging to attain, meaning computational models of the receptor and ligand binding mode are sometimes necessary. This leads to uncertainty in understanding ligand\textendash protein binding induced changes such as, water positioning and displacement, side chain positioning, hydrogen bond networks, and the overall structure of the hydration shell around the ligand and protein. In other words, the very elements that define structure activity relationships (SARs) and are crucial for accurate binding free energy calculations are typically more uncertain for GPCRs. In this work we use free energy perturbation (FEP) to predict the relative binding free energies for ligands of two different GPCRs. We pinpoint the key aspects for success such as the important role of key water molecules, amino acid ionization states, and the benefit of equilibration with specific ligands. Initial calculations following typical FEP setup and execution protocols delivered no correlation with experiment, but we show how results are improved in a logical and systematic way. This approach gave, in the best cases, a coefficient of determination (R2) compared with experiment in the range of 0.6\textendash 0.9 and mean unsigned errors compared to experiment of 0.6\textendash 0.7 kcal/mol. We anticipate that our findings will be applicable to other difficult-to-model protein ligand data sets and be of wide interest for the community to continue improving FE binding energy predictions.",
    journal = "Journal of Chemical Information and Modeling",
    url = "http://dx.doi.org/10.1021/acs.jcim.0c00449"
}

@article{dixon2018predicting,
    author = "Dixon, Tom and Lotz, Samuel D. and Dickson, Alex",
    title = "Predicting ligand binding affinity using on- and off-rates for the SAMPL6 SAMPLing challenge",
    year = "2018",
    month = "October",
    volume = "32",
    pages = "1001--1012",
    issn = "0920-654X",
    doi = "10.1007/s10822-018-0149-3",
    abstract = "Interest in ligand binding kinetics has been growing rapidly, as it is being discovered in more and more systems that ligand residence time is the crucial factor governing drug efficacy. Many enhanced sampling methods have been developed with the goal of predicting ligand binding rates (\$\$k\\_\{\textbackslash text \{on\}\}\$\$kon) and/or ligand unbinding rates (\$\$k\\_\{\textbackslash text \{off\}\}\$\$koff) through explicit simulation of ligand binding pathways, and these methods work by very different mechanisms. Although there is not yet a blind challenge for ligand binding kinetics, here we take advantage of experimental measurements and rigorously computed benchmarks to compare estimates of \$\$K\\_D\$\$KDcalculated as the ratio of two rates: \$\$k\\_\{\textbackslash text \{off\}\}/k\\_\{\textbackslash text \{on\}\}\$\$koff/kon. These rates were determined using a new enhanced sampling method based on the weighted ensemble framework that we call ``REVO'': Reweighting of Ensembles by Variance Optimization. This is a further development of the WExplore enhanced sampling method, in which trajectory cloning and merging steps are guided not by the definition of sampling regions, but by maximizing trajectory variance. Here we obtain estimates of \$\$k\\_\{\textbackslash text \{on\}\}\$\$konand \$\$k\\_\{\textbackslash text \{off\}\}\$\$koffthat are consistent across multiple simulations, with an average log10-scale standard deviation of 0.28 for on-rates and 0.56 for off-rates, which is well within an order of magnitude and far better than previously observed for previous applications of the WExplore algorithm. Our rank ordering of the three host\textendash guest pairs agrees with the reference calculations, however our predicted \$\$\textbackslash Delta G\$\${$\Delta$}Gvalues were systematically lower than the reference by an average of 4.2\textasciitilde kcal/mol. Using tree network visualizations of the trajectories in the REVO algorithm, and conformation space networks for each system, we analyze the results of our sampling, and hypothesize sources of discrepancy between our \$\$K\\_D\$\$KDvalues and the reference. We also motivate the direct inclusion of \$\$k\\_\{\textbackslash text \{on\}\}\$\$konand \$\$k\\_\{\textbackslash text \{off\}\}\$\$koffchallenges in future iterations of SAMPL, to further develop the field of ligand binding kinetics prediction and modeling.",
    journal = "Journal of Computer-Aided Molecular Design",
    language = "en",
    number = "10",
    url = "http://dx.doi.org/10.1007/s10822-018-0149-3"
}

@article{doerr2014onthefly,
