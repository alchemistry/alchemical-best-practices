    file = "/Users/toni\_brain/Zotero/storage/K2N8LVKY/Woods et al. - 2003 - The Development of Replica-Exchange-Based Free-Ene.pdf;/Users/toni\_brain/Zotero/storage/AFI535QC/jp0356620.html",
    journal = "The Journal of Physical Chemistry B",
