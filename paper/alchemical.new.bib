@article{clark2019relative,
    author = "Clark, Anthony J. and Negron, Christopher and Hauser, Kevin and Sun, Mengzhen and Wang, Lingle and Abel, Robert and Friesner, Richard A.",
    title = "Relative Binding Affinity Prediction of Charge-Changing Sequence Mutations with FEP in Protein–Protein Interfaces",
