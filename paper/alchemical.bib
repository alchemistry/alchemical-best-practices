  file = {/Users/toni_brain/Zotero/storage/DTH7QEY5/Bosisio et al. - 2016 - Blinded predictions of distribution coefficients i.pdf},
  journal = {J. Comput. Aided Mol. Des.},
  keywords = {Distribution coefficient,logDlog⁡D\\log \\hbox {D},SAMPL5},
  language = {en},
  number = {11}
}

