@article{hahn2019alchemical,
  title = {Alchemical {{Free}}-{{Energy Calculations}} by {{Multiple}}-{{Replica}} {$\lambda$}-{{Dynamics}}: {{The Conveyor Belt Thermodynamic Integration Scheme}}},
  shorttitle = {Alchemical {{Free}}-{{Energy Calculations}} by {{Multiple}}-{{Replica}} {$\lambda$}-{{Dynamics}}},
  author = {Hahn, David F. and H{\"u}nenberger, Philippe H.},
  year = {2019},
