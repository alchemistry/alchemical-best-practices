@article{brandt2011congeneric,
  title = {Congeneric but {{Still Distinct}}: {{How Closely Related Trypsin Ligands Exhibit Different Thermodynamic}} and {{Structural Properties}}},
  shorttitle = {Congeneric but {{Still Distinct}}},
  author = {Brandt, Tobias and Holzmann, Nicole and Muley, Laveena and Khayat, Maan and {Wegscheid-Gerlach}, Christof and Baum, Bernhard and Heine, Andreas and Hangauer, David and Klebe, Gerhard},
  year = {2011},
  month = feb,
  volume = {405},
  pages = {1170--1187},
  issn = {0022-2836},
  doi = {10.1016/j.jmb.2010.11.038},
  abstract = {A congeneric series of benzamidine-type ligands with a central proline moiety and a terminal cycloalkyl group\textemdash linked by a secondary amine, ether, or methylene bridge\textemdash was synthesized as trypsin inhibitors. This series of inhibitors was investigated by isothermal titration calorimetry, crystal structure analysis in two crystal forms, and molecular dynamics simulations. Even though all of these congeneric ligands exhibited essentially the same affinity for trypsin, their binding profiles at the structural, dynamic, and thermodynamic levels are very distinct. The ligands display a pronounced enthalpy/entropy compensation that results in a nearly unchanged free energy of binding, even though individual enthalpy and entropy terms change significantly across the series. Crystal structures revealed that the secondary amine-linked analogs scatter over two distinct conformational families of binding modes that occupy either the inside or of the outside the protein's S3/S4 specificity pocket. In contrast, the ether-linked and methylene-linked ligands preferentially occupy the hydrophobic specificity pocket. This also explains why the latter ligands could only be crystallized in the conformationally restricting closed crystal form whereas the derivative with the highest residual mobility in the series escaped our attempts to crystallize it in the closed form; instead, a well-resolved structure could only be achieved in the open form with the ligand in disordered orientation. These distinct binding modes are supported by molecular dynamics simulations and correlate with the shifting enthalpic/entropic signatures of ligand binding. The examples demonstrate that, at the molecular level, binding modes and thermodynamic binding signatures can be very different even for closely related ligands. However, deviating binding profiles provide the opportunity to optimally address a given target.},
  file = {/Users/toni_brain/Zotero/storage/QY5HGRXN/S0022283610012684.html},
  journal = {J. Mol. Biol.},
  keywords = {crystal structure analysis,enthalpy/entropy compensation,isothermal titration calorimetry,molecular dynamics simulations,protein–ligand interactions},
  language = {en},
  number = {5}
}

